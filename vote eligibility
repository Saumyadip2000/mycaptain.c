// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int age,citizen;
    cout<<"Enter your age:";
    cin>>age;
    cout<<"Enter whether INDIAN citizen or not:\n 1-YES\n 2-NO\n";
    cin>>citizen;
    if(age>=18 && citizen==1)
    {
        cout<<"You are eligible for vote......\n";
    }
    else 
    {
        cout<<"You are not eligible for vote.....";
    }

    return 0;
}
